    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "poker.local"
            ]
        },
        {
            "name": "Launch server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "env": {
                "ROOM_LIST": "Fire,Water,Air,Earth"
            },
            "program": "${workspaceFolder}/cmd",
            "args": [
                "server",
                "-debug"
            ]
        },
    ]
}